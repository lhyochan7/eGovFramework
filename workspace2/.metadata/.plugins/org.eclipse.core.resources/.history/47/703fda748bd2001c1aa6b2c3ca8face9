<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>

<%
	String num = request.getParameter("num");
	if(num == null || num.equals("")) {
		num = "10";
	}

	String result = "";
	for(int i=0; i< Integer.parseInt(num); i++) { 
	
		result += "hello~~~~";
	
	}

 %>

<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<%= result %>
</body>
</html>